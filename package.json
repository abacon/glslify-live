{
  "name": "glslify-live",
  "version": "0.0.0",
  "description": "A browserify transform that transparently enables live reloading of your shaders when using glslify.",
  "main": "transform.js",
  "browser": "browser.js",
  "scripts": {
    "test": "cd test && beefy index.js --open -- -t ../ -t glslify",
    "start": "node server.js"
  },
  "author": "Hugh Kennedy <hughskennedy@gmail.com> (http://hughsk.io/)",
  "license": "MIT",
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "dependencies": {
    "bl": "^0.8.0",
    "chokidar": "^0.8.2",
    "envify": "^1.2.1",
    "esprima": "^1.2.2",
    "events": "^1.0.1",
    "gl-shader-update": "0.0.0",
    "glsl-deparser": "0.0.3",
    "glsl-resolve": "0.0.1",
    "glslify": "^1.3.0",
    "glslify-stream": "^0.2.1",
    "replace-method": "0.0.0",
    "request": "^2.36.0",
    "resolve": "^0.7.0",
    "sleuth": "^0.1.0",
    "sse-stream": "0.0.4",
    "static-eval": "^0.2.2",
    "through": "^2.3.4",
    "uniq": "^1.0.1",
    "xhr": "^1.7.0"
  },
  "devDependencies": {
    "a-big-triangle": "0.0.0",
    "beefy": "^1.1.0",
    "gl-context": "^0.1.0",
    "glslify": "^1.3.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hughsk/glslify-live.git"
  },
  "keywords": [
    "glsl",
    "live",
    "reload",
    "update",
    "webgl",
    "shader",
    "fragment",
    "vertex",
    "refresh",
    "browserify-transform"
  ],
  "bugs": {
    "url": "https://github.com/hughsk/glslify-live/issues"
  },
  "homepage": "https://github.com/hughsk/glslify-live"
}
